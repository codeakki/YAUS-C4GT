{
  "name": "remove-undefined-objects",
  "description": "Remove undefined properties and empty arrays from an object",
  "version": "2.0.0",
  "main": "dist/index.js",
  "author": "Jon Ursenbach <jon@ursenba.ch>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/readmeio/remove-undefined-objects.git"
  },
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --ext .js,.ts",
    "prebuild": "rm -rf dist/ @types/",
    "prepack": "npm run build",
    "prepare": "husky install",
    "pretest": "npm run lint",
    "prettier": "prettier --list-different --write \"./**/**.{js,ts}\"",
    "release": "npx conventional-changelog-cli -i CHANGELOG.md -s && git add CHANGELOG.md",
    "test": "tsc; jest --coverage"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.0.0",
    "@readme/eslint-config": "^8.7.2",
    "@types/jest": "^27.4.0",
    "eslint": "^8.14.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "prettier": "^2.6.2",
    "ts-jest": "^27.1.3",
    "typescript": "^4.5.5"
  },
  "prettier": "@readme/eslint-config/prettier",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "body-max-line-length": [
        0,
        "always",
        "Infinity"
      ],
      "footer-max-line-length": [
        0,
        "always",
        "Infinity"
      ]
    }
  }
}
